<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
            http://www.springframework.org/schema/beans/spring-beans.xsd
            http://www.springframework.org/schema/context
            http://www.springframework.org/schema/context/spring-context-3.0.xsd">


	<bean id="stuBean" class="com.spring.di.Student">
		<property name="sid" value="101"></property>
		<property name="name" value="Dinga"></property>
	</bean>

	<bean id="personBean" class="com.spring.di.Person">
		<constructor-arg value="111"></constructor-arg>
		<constructor-arg value="Manga"></constructor-arg>
		<constructor-arg value="Thane"></constructor-arg>
	</bean>

</beans>